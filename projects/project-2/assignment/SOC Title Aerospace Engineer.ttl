@prefix ex: <http://example.org/ontology#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix schema: <https://schema.org/> .

#################################################################
# --- Occupation: Aerospace Engineer ---
#################################################################

ex:Aerospace_Engineer
    rdf:type schema:Occupation ;
    schema:name "Aerospace Engineer" ;
    schema:occupationalCategory "17-2010" ;     # Example SOC code
    schema:identifier "17-2010" ;              # Duplicate for clarity
    ex:hasSOCCode "17-2010" ;
    rdfs:subClassOf ex:Engineer ;
    ex:hasSubclassSOCCode ex:SOC_17_2011 , ex:SOC_17_3021 ;
    ex:inheresIn ex:Act_of_Aircraft_Processing .

#################################################################
# --- SOC Subclasses ---
#################################################################

ex:SOC_17_2011
    rdf:type ex:SOCSubclass ;
    rdfs:label "17-2011" ;
    ex:inheresIn ex:Aircraft_Missile_Spacecraft_Design ,
                 ex:Aircraft_Missile_Spacecraft_Construction ,
                 ex:Aircraft_Missile_Spacecraft_Testing ,
                 ex:Adaptability_Evaluation .

ex:SOC_17_3021
    rdf:type ex:SOCSubclass ;
    rdfs:label "17-3021" ;
    ex:isParticipantOf ex:Operate_Install_Adjust_Maintain_Systems ;
    ex:inheresInActOfMeasuring ex:Testing_of_Measurement_Instruments ;
    ex:inheresIn ex:Data_Interpretation_and_Analysis .

#################################################################
# --- Processes & Outputs ---
#################################################################

ex:Act_of_Aircraft_Processing rdf:type ex:Process ;
    rdfs:label "Act of Aircraft Processing"@en .

ex:Aircraft_Missile_Spacecraft_Design rdf:type ex:DesignProcess ;
    rdfs:label "Aircraft, Missile, Spacecraft Design"@en .

ex:Aircraft_Missile_Spacecraft_Construction rdf:type ex:ConstructionProcess ;
    rdfs:label "Aircraft, Missile, Spacecraft Construction"@en .

ex:Aircraft_Missile_Spacecraft_Testing rdf:type ex:TestingProcess ;
    rdfs:label "Aircraft, Missile, Spacecraft Testing"@en .

ex:Adaptability_Evaluation rdf:type ex:EvaluationProcess ;
    rdfs:label "Adaptability Evaluation"@en ;
    ex:produces ex:Changes_in_Design_and_Manufacturing_Processes .

ex:Changes_in_Design_and_Manufacturing_Processes
    rdf:type ex:ProcessOutput ;
    ex:isOutputOf ex:Adaptability_Evaluation ;
    ex:isAbout ex:Aircraft_Missile_Spacecraft_Design_Improvements .

ex:Aircraft_Missile_Spacecraft_Design_Improvements
    rdf:type ex:DesignImprovement ;
    ex:isAbout ex:Artifact_Design .

ex:Artifact_Design rdf:type ex:DesignConcept .

ex:Operate_Install_Adjust_Maintain_Systems
    rdf:type ex:OperationalActivity ;
    rdfs:label "Operate, install, adjust, and maintain integrated computer/communications systems, consoles, simulators, and other data acquisition systems"@en .

ex:Testing_of_Measurement_Instruments
    rdf:type ex:MeasurementProcess ;
    rdfs:label "Testing of measurement instruments and equipment to launch, track, position, and evaluate air and space vehicles"@en ;
    ex:realizes ex:Instruments_Used_to_Launch_Track_Position_Evaluate_Vehicles .

ex:Instruments_Used_to_Launch_Track_Position_Evaluate_Vehicles
    rdf:type ex:Instrument ;
    rdfs:label "Instruments and equipment used to launch, track, position, and evaluate air and space vehicles"@en .

ex:Data_Interpretation_and_Analysis
    rdf:type ex:AnalyticalProcess ;
    rdfs:label "Data interpretation and analysis"@en .
