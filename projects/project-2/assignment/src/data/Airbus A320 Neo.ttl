#################################################################
## AIRCRAFT ENTITY â€” MATERIAL ENTITY / ARTIFACT
#################################################################

ex:A320Neo rdf:type cco:FixedWingAircraft ;
    rdfs:label "Airbus A320 Neo" ;
    cco:hasContinuantPart ex:JetEngine_x2 ;
    cco:isClassifiedBy ex:ICAO_A20N,
                       ex:FAA_A20N_1,
                       ex:FAA_A20N_2,
                       ex:Manufacturer_Airbus,
                       ex:MainGearConfig_D,
                       ex:ICAO_WTC_Medium,
                       ex:FAAWeight_Large,
                       ex:CWT_F,
                       ex:Wake_OneHalf_D,
                       ex:Wake_AppxA_D,
                       ex:Wake_AppxB_C,
                       ex:SRS_III,
                       ex:LAHSO_7,
                       ex:SFAA_Registry_Yes ;
    cco:hasQuality ex:Q_ApproachSpeed ,
                   ex:Q_Wingspan ,
                   ex:Q_Length ,
                   ex:Q_TailHeight ,
                   ex:Q_WheelBase ,
                   ex:Q_CockpitToMainGear ,
                   ex:Q_MainGearWidth ,
                   ex:Q_MTOW ,
                   ex:Q_MALW ,
                   ex:Q_ParkingArea ;
    ex:hasRegistrationCount ex:RegCount_147 ;
    ex:hasTMFSOperationsFY24 ex:TMFS_540276 ;
    cco:hasDescription ex:Remark_FSB_Validation .

#################################################################
## CONTINUANT PARTS
#################################################################

ex:JetEngine_x2 rdf:type cco:Engine ;
    rdfs:label "Jet Engine - x2" .

