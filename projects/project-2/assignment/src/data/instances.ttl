data:Airbus_A320n_Spec_2
    a               owl:NamedIndividual ;
    a               [ a                 owl:Restriction ;
                      owl:allValuesFrom [ a                  owl:Class ;
                                          owl:intersectionOf ( model:Fixed_Wing [ a              owl:Restriction ;
                                                                                  owl:hasValue   data:maximum_knot_approach_speed_ice ;
                                                                                  owl:onProperty cco:ont00001920 ] ) ] ;
                      owl:onProperty    cco:ont00001942 ] ;
    rdfs:label      "Airbus A321-111", "Airbus A320n Spec 2" ;
    skos:definition "Airbus A321-111", "Airbus A320n Spec 2" ;
    cco:ont00001942 data:Airbus_A321_111_1 .

data:maximum_knot_approach_speed_ice
    rdf:type                              cco:ont00000127 ;
    model:has_maximum_knot_approach_speed "142"^^xsd:integer ;
    rdfs:label                            "maximum knot approach speed of 142" .

data:Airbus_A321_111_1
    rdf:type        owl:NamedIndividual, model:Fixed_Wing ;
    cco:ont00001920 data:maximum_knot_approach_speed_ice ;
    rdfs:label      "Airbus A321-111 1" .

data:Measurement_Unit_of_Knots
    rdf:type        owl:NamedIndividual, cco:ont00000120 ;
    cco:ont00001961 data:record_3 ;
    cco:ont00001961 data:maximum_knot_approach_speed_ice ;
    rdfs:label      "Measurement Unit of Knots" .

data:record_3
    rdf:type                    owl:NamedIndividual, cco:ont00000253 ;
    model:has_measurement_value "142"^^xsd:decimal ;
    rdfs:label                  "Prescribed Average Maximum Knot Approach Speed IBE" .

