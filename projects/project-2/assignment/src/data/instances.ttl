# Particulars
#################################################################

data:airbus
    rdf:type   owl:NamedIndividual ;
    rdfs:label "Airbus" .

data:Airbus_320N_Spec_1
    a          owl:NamedIndividual ;
    a          [ a                 owl:Restriction ;
                 owl:allValuesFrom [ a                  owl:Class ;
                                     owl:intersectionOf ( model:Fixed_Wing [ a              owl:Restriction ;
                                                                             owl:hasValue   data:airbus ;
                                                                             owl:onProperty model:has_Manufacturer ]
                                                          [ a                        owl:Restriction ;
                                                            owl:onClass              model:Jet_Engine ;
                                                            owl:onProperty           bfo:BFO_0000178 ;
                                                            owl:qualifiedCardinality "2"^^xsd:nonNegativeInteger ]
                                                          [ a              owl:Restriction ;
                                                            owl:hasValue   "123.3"^^xsd:decimal ;
                                                            owl:onProperty model:has_Length_Ft ] [ a owl:Restriction ;
                                                                                                   owl:hasValue
                                                                                                     "BADA Airbus A320-271N" ;
                                                                                                   owl:onProperty
                                                                                                     model:has_Model_BADA ]
                                                          [ a              owl:Restriction ;
                                                            owl:hasValue   "FAA Airbus A320 Neo" ;
                                                            owl:onProperty model:has_Model_FAA ] [ a owl:Restriction ;
                                                                                                   owl:hasValue
                                                                                                     "39.6"^^xsd:decimal ;
                                                                                                   owl:onProperty
                                                                                                     model:has_Tail_Height_Ft ] ) ] ;
                 owl:onProperty    cco:ont00001808 ] ;
    rdfs:label "Airbus A320n Spec 1" .

data:Airbus_A320_NEO
    rdf:type                 owl:NamedIndividual, model:Fixed_Wing ;
    obo:BFO_0000178          data:Jet_Engine_1, data:Jet_Engine_2 ;
    model:has_Length_Ft      "123.3"^^xsd:decimal ;
    model:has_Manufacturer   data:airbus ;
    model:has_Tail_Height_Ft "39.6"^^xsd:decimal ;
    model:has_Model_BADA     "Airbus A320-271N" ;
    model:has_Model_FAA      "Airbus A320 Neo" ;
    rdfs:label               "Airbus A320 NEO" .

data:model_BADA_Airbus_A320-271N
    rdf:type   owl:NamedIndividual, model:Model_BADA ;
    rdfs:label "BADA Airbus A320-271N" .

data:model_FAA_Airbus_A320_Neo
    rdf:type   owl:NamedIndividual, model:Model_FAA ;
    rdfs:label "FAA Airbus A320 Neo" .

data:Jet_Engine_1
    rdf:type   owl:NamedIndividual, model:Jet_Engine ;
    rdfs:label "Jet Engine" .

data:Jet_Engine_2
    rdf:type   owl:NamedIndividual, model:Jet_Engine ;
    rdfs:label "Jet Engine" .

