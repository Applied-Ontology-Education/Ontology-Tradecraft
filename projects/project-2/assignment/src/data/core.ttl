# Universals
#################################################################

:Fixed_Wing
    rdf:type        owl:Class ;
    rdfs:subClassOf bfo:BFO_0000040 ;
    rdfs:label      "Fixed Wing" ;
    skos:definition "A Fixed wing is a manufacture aircraft object." .

:Jet_Engine
    rdf:type        owl:Class ;
    rdfs:subClassOf bfo:BFO_0000040 ;
    rdfs:label      "Jet Engine" ;
    skos:definition "A Jet Engine is a manufactured object." .

:Model_BADA
    rdf:type        owl:Class ;
    rdfs:subClassOf bfo:BFO_0000031 ;
    rdfs:label      "Model BADA" ;
    skos:definition "Model BADA is the model type designator of an airplane according to BADA." .

:Model_FAA
    rdf:type        owl:Class ;
    rdfs:subClassOf bfo:BFO_0000031 ;
    rdfs:label      "Model FAA" ;
    skos:definition "Model FAA is the model type designator of an airplane according to FAA." .

# Object properties
#################################################################

:has_Manufacturer
    rdf:type        owl:ObjectProperty ;
    rdfs:label      "has Manufacturer" ;
    skos:definition "Every artifact has a manufacturer." .


# Data properties
#################################################################

:has_Length_Ft
    rdf:type           owl:DatatypeProperty ;
    rdfs:subPropertyOf owl:topDataProperty ;
    rdfs:label         "has Length Ft" ;
    skos:definition    "An fixed wing airplane has a determinable length." .

:has_Tail_Height_Ft
    rdf:type           owl:DatatypeProperty ;
    rdfs:subPropertyOf owl:topDataProperty ;
    rdfs:label         "has Tail Height Ft" ;
    skos:definition    "An fixed wing airplane has a determinable tail height." .


:has_Model_BADA
    rdf:type           owl:DatatypeProperty ;
    rdfs:subPropertyOf owl:topDataProperty ;
    rdfs:label         "has Model BADA" ;
    skos:definition    "An model of a fixed wing airplane is assigned a BADA code." .

:has_Model_FAA
    rdf:type        owl:DatatypeProperty ;
    rdfs:label      "has Model FAA" ;
    skos:definition "An model of a fixed wing airplane is assigned a FAA code." .

